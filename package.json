{
  "name": "snakeonlinemulti",
  "version": "1.0.0",
  "description": "Simple snake game using html Canvas",
  "repository": "github:tursoft/snakeonlinemulti",
  "scripts": {
    "build": "cd ./client & ng build --prod",
    "start": "cd ./client & npm run start",

    "frontend": "cd ./client & npm run start",
    "backend": "cd ./server & npm run start",
    
    "all": "cd ./client & npm run start; cd ./server & npm run start",
    
    "helm": "helm package ./charts/snakeonlinemulti -d ./charts",
    "helm-install": "helm install snakeonlinemulti ./charts/snakeonlinemulti-1.0.0.tgz"
  },
  "author": "Muhammet TURÅžAK <tursoft@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "i": "^0.3.6",
    "npm": "^6.14.5"
  }
}
